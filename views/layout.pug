doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    title Members Only Message Board #{title ? `- ${title}` : ''}
  body 
    if !user
      include login.pug
    else 
      include logout.pug
    div.content
      block content